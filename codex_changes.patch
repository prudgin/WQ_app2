+            "InvariantScript": "Set(varSaveError, \"\");\n\nIf(\n    IsBlank(varMyFarm),\n    Set(varSaveError, \"Farm is not set. Go to Home, select a farm, then press Save settings.\")\n);\n\nIf(\n    IsBlank(varSaveError) && IsBlank(ddSite.Selected.Site),\n    Set(varSaveError, \"Please select a Site.\")\n);\n\nIf(\n    IsBlank(varSaveError) && IsBlank(ddUnit.Selected.UnitId),\n    Set(varSaveError, \"Please select a Unit.\")\n);\n\nIf(\n    IsBlank(varSaveError) && IsBlank(Trim(txtInitials.Text)),\n    Set(varSaveError, \"Please enter initials.\")\n);\n\nIf(\n    IsBlank(varSaveError) && IsBlank(dpReadingDate.SelectedDate),\n    Set(varSaveError, \"Please select a date.\")\n);\n\nIf(\n    IsBlank(varSaveError) &&\n    !(\n        IsMatch(txtReadingTime.Text, \"^\\d\\d:\\d\\d$\") &&\n        Value(Left(txtReadingTime.Text, 2)) < 24 &&\n        Value(Mid(txtReadingTime.Text, 4, 2)) < 60\n    ),\n    Set(varSaveError, \"Time must be in HH:mm (e.g., 07:30).\")\n);\n\n/* Numeric validation (only when a value is provided) */\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtAlkalinity.Text))   && !IsNumeric(txtAlkalinity.Text),   Set(varSaveError, \"Alkalinity must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtGHardness.Text))    && !IsNumeric(txtGHardness.Text),    Set(varSaveError, \"G HARD must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtpH.Text))           && !IsNumeric(txtpH.Text),           Set(varSaveError, \"pH must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtPhosphorus.Text))   && !IsNumeric(txtPhosphorus.Text),   Set(varSaveError, \"PHOS must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtTAN.Text))          && !IsNumeric(txtTAN.Text),          Set(varSaveError, \"AMMO (TAN) must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtNitrite.Text))      && !IsNumeric(txtNitrite.Text),      Set(varSaveError, \"NITRITE (NO2) must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtNitrate.Text))      && !IsNumeric(txtNitrate.Text),      Set(varSaveError, \"NITRATE (NO3) must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtFreeCopper1.Text))  && !IsNumeric(txtFreeCopper1.Text),  Set(varSaveError, \"Free Copper 1 must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtFreeCopper2.Text))  && !IsNumeric(txtFreeCopper2.Text),  Set(varSaveError, \"Free Copper 2 must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtTurbidityNTU.Text)) && !IsNumeric(txtTurbidityNTU.Text), Set(varSaveError, \"Turbidity must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtSalinity.Text))     && !IsNumeric(txtSalinity.Text),     Set(varSaveError, \"Salinity must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtCaHardness.Text))   && !IsNumeric(txtCaHardness.Text),   Set(varSaveError, \"Ca Hardness must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtChloride.Text))     && !IsNumeric(txtChloride.Text),     Set(varSaveError, \"Chloride must be a number.\"));\nIf(IsBlank(varSaveError) && !IsBlank(Trim(txtTemperature.Text))  && !IsNumeric(txtTemperature.Text),  Set(varSaveError, \"Temperature must be a number.\"));\n\nIf(\n    IsBlank(varSaveError) &&\n    IsBlank(Trim(txtAlkalinity.Text)) &&\n    IsBlank(Trim(txtGHardness.Text)) &&\n    IsBlank(Trim(txtpH.Text)) &&\n    IsBlank(Trim(txtPhosphorus.Text)) &&\n    IsBlank(Trim(txtTAN.Text)) &&\n    IsBlank(Trim(txtNitrite.Text)) &&\n    IsBlank(Trim(txtNitrate.Text)) &&\n    IsBlank(Trim(txtFreeCopper1.Text)) &&\n    IsBlank(Trim(txtFreeCopper2.Text)) &&\n    IsBlank(Trim(txtTurbidityNTU.Text)) &&\n    IsBlank(Trim(txtSalinity.Text)) &&\n    IsBlank(Trim(txtCaHardness.Text)) &&\n    IsBlank(Trim(txtChloride.Text)) &&\n    IsBlank(Trim(txtTemperature.Text)),\n    Set(varSaveError, \"Please enter at least one reading value.\")\n);\n\n\nIf(\n    !IsBlank(varSaveError),\n    Notify(varSaveError, NotificationType.Error),\n\n    